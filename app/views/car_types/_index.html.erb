<% @car_types.each do |car_type| %>
<tr >
  <td><%= car_type.name %></td>
  <td class="actions right-align">
    <%= link_to raw("<i class='material-icons circle'>mode_edit</i>"), edit_car_type_path(car_type), remote: true %>
    <%= link_to raw("<i class='material-icons circle'>cancel</i>"), car_type_delete_path(car_type), remote: true %>
  </td>
</tr>
<% end %>
