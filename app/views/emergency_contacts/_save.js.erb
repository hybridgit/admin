$("ul.errors").html("")
<% if @emergency_contact.errors.any? %>
  <% @emergency_contact.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li/>").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $("#emergency-contacts-list tbody").html("<%= escape_javascript(render 'index') %>")
  $("#emergency-contact-modal").closeModal()
<% end %>
